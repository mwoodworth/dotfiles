alias h='history'
alias vi='vim'
alias gvi='gvim'
alias tc='truecrypt'
alias go='gnome-open'
alias xv='xview'
alias hgrep='history | grep'
alias apt-search='apt-cache search'
alias net-list='lsof -Pnl +M -i4'
alias mv-py='mv ~/.vim/bundle/python-mode ~/'
alias umv-py='mv ~/python-mode ~/.vim/bundle/'
alias net-list-5000='net-list | grep 5000'
